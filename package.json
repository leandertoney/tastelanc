{
  "name": "tastelanc",
  "private": true,
  "scripts": {
    "dev:web": "pnpm --filter @tastelanc/web dev",
    "dev:mobile": "pnpm --filter @tastelanc/mobile start",
    "build:web": "pnpm --filter @tastelanc/web build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "engines": {
    "node": ">=18"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "18.3.12",
      "@types/react-dom": "18.3.1"
    }
  }
}
