# TasteLanc Environment Variables
# Copy this to .env and fill in your values

# Supabase Configuration
EXPO_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
EXPO_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Feature Flags
# Tier-based content gating (hide premium content for Basic tier restaurants)
# Set to 'true' to enable in Expo Go for testing
# Set to 'false' or omit for production (default behavior)
EXPO_PUBLIC_ENABLE_TIER_GATING=false
